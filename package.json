{
  "name": "cv-sveltekit",
  "version": "0.1.0",
  "type": "module",
  "volta": {
    "node": "22.13.1",
    "pnpm": "10.6.5"
  },
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "husky",
    "preinstall": "node ./scripts/preinstall.js",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint:eslint": "eslint ./**/*.{js,jsx,ts,tsx,svelte} --cache --fix",
    "lint:style": "stylelint ./**/*.{css,scss,svelte} --cache --fix",
    "format": "prettier --write .",
    "lint:lint-staged": "lint-staged",
    "test:unit": "vitest",
    "cz": "czg",
    "log": "conventional-changelog -p cmyr-config -i CHANGELOG.md -s -r 0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.3.0",
    "@eslint/js": "^9.6.0",
    "@sveltejs/adapter-node": "^5.2.2",
    "@sveltejs/kit": "^2.8.3",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@types/eslint": "^8.56.7",
    "@unocss/eslint-config": "^0.62.3",
    "@unocss/extractor-svelte": "^0.62.3",
    "@unocss/reset": "66.1.0-beta.6",
    "autoprefixer": "^10.4.19",
    "conventional-changelog-cli": "^5.0.0",
    "cssnano": "^7.0.1",
    "czg": "^1.11.1",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.43.0",
    "globals": "^16.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.39",
    "postcss-html": "^1.7.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "sass": "^1.77.6",
    "stylelint": "^16.9.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^5.1.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-prettier": "^5.0.2",
    "svelte": "^4.2.19",
    "svelte-check": "^3.8.5",
    "svelte-i18n": "^4.0.0",
    "tslib": "^2.7.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0",
    "unocss": "^0.62.3",
    "vite": "^5.4.6"
  }
}
